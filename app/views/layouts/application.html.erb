  <style type="text/css">
    @import "http://ajax.googleapis.com/ajax/libs/dojo/1.9.2/dijit/themes/claro/claro.css";
    @import "http://ajax.googleapis.com/ajax/libs/dojo/1.9.2/dijit/themes/dijit.css";
  </style>
</head>
<body class="claro">
<%= yield %>
</body>
</html>
